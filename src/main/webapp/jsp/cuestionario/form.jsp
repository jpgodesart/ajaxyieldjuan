<form class="form-horizontal" action="#" id="formularioCuestionario" name="formularioCuestionario">
    <h2>Cuestionario</h2>
    <div class="control-group">
        <label class="control-label" for="inputId">Id:</label>
        <div class="controls">
            <input type="text" id="id" name="id" placeholder="id" />
        </div>
    </div>
    <div class="control-group">
        <label class="control-label"  for="inputNombre">Descripción:</label>
        <div class="controls">
            <input type="text" id="descripcion" name="descripcion" size="15" placeholder="descripción" />
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="fecha" >Fecha:</label>
        <div class="controls">
            <input type="text" id="fecha" name="fecha" placeholder="fecha" size="15" />
        </div>
    </div>
    <div class="control-group">
        <label class="control-label"  for="evaluacion">Evaluación:</label>
        <div class="controls">
            <input type="text" id="evaluacion" name="evaluacion" placeholder="evaluación" size="15" />
        </div>
    </div>
    <div class="control-group">
        <label class="control-label"  for="activo">Activo:</label>
        <div class="controls">
            <input type="text" id="activo" name="activo" placeholder="activo" size="15" />
        </div>
    </div>
    <div class="control-group">
        <div class="controls">
            <button type="submit" id="submitForm" class="btn submitForm">Submit</button>
        </div>
    </div>
</form>
